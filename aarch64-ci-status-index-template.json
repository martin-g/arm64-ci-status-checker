{
    "index_patterns": ["aarch64-ci-status-*"],
    "priority" : 1,
    "template": {
        "settings": {
           "number_of_shards": 1
        },
        "mappings": {
            "properties": {
                "project_name": {
                    "type": "keyword"
                },
                "status": {
                    "type": "keyword"
                },
                "ci_provider": {
                    "type": "keyword"
                },
                "timestamp": {
                    "type": "date"
                }
            }
        }
    }
}